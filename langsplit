import sys
import re

def main():
    for line in sys.stdin:
        line = line.strip()
        # 英語（ASCII文字）の数をカウント
        english_count = len(re.findall(r'[a-zA-Z0-9]', line))
        # 全体の文字数
        total_count = len(line)

        if total_count > 0 and (english_count / total_count) >= 0.5:
            print(line, file=sys.stdout)
        else:
            print(line, file=sys.stderr)

if __name__ == '__main__':
    main()
